{
  "name": "active-inflector",
  "version": "0.1.0",
  "type": "module",
  "description": "active-inflector goal is to be rails compatible.",
  "keywords": [
    "inflector",
    "inflection",
    "string",
    "active-record",
    "rails",
    "active-inflection",
    "active",
    "plural",
    "pluralize",
    "pluralization",
    "singular",
    "singularize",
    "singularization"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/NullVoxPopuli/active-inflector.git"
  },
  "exports": {
    ".": {
      "types": "./declarations/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "module": "./dist/index.js",
  "main": "./dist/index.js",
  "types": "./declarations/index.d.ts",
  "files": [
    "dist",
    "declarations"
  ],
  "license": "MIT",
  "author": "NullVoxPopuli",
  "scripts": {
    "release": "changeset publish",
    "start": "pnpm build:js --watch",
    "build:js": "./build.sh",
    "build:types": "tsc --emitDeclarationOnly",
    "prepack": "pnpm turbo build",
    "check": "turbo lint",
    "fix": "turbo lint:fix",
    "lint:js": "eslint .",
    "lint:prettier": "prettier --check .",
    "lint:js:fix": "eslint . --fix",
    "lint:prettier:fix": "prettier --write .",
    "lint:types": "tsc --noEmit",
    "lint:package": "publint",
    "lint:published-types": "attw --pack --ignore-rules cjs-resolves-to-esm --ignore-rules internal-resolution-error",
    "test": "vitest --coverage"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.13.0",
    "@changesets/cli": "^2.26.2",
    "@nullvoxpopuli/eslint-configs": "^3.2.0",
    "@tsconfig/esm": "^1.0.4",
    "@tsconfig/strictest": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "esbuild": "^0.20.0",
    "eslint": "^8.47.0",
    "prettier": "^3.0.2",
    "publint": "^0.2.0",
    "spack": "^1.0.0",
    "turbo": "^1.10.12",
    "typescript": "^5.1.6",
    "vitest": "^1.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "volta": {
    "node": "20.17.0",
    "pnpm": "9.9.0"
  },
  "dependencies": {
    "capital-case": "^1.0.4"
  }
}
