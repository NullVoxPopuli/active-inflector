{
  "name": "active-inflector",
  "version": "0.0.0",
  "type": "module",
  "description": "active-inflector goal is to be rails compatible.",
  "keywords": [
    "ember-addon",
    "inflector",
    "inflection",
    "string",
    "active-record",
    "rails",
    "active-inflection",
    "active",
    "plural",
    "pluralize",
    "pluralization",
    "singular",
    "singularize",
    "singularization"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/NullVoxPopuli/active-inflector.git"
  },
  "license": "MIT",
  "author": "NullVoxPopuli",
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "turbo lint",
    "lint:fix": "turbo lint:fix",
    "lint:js": "eslint .",
    "lint:prettier": "prettier --check .",
    "lint:js:fix": "eslint . --fix",
    "lint:prettier:fix": "prettier --write .",
    "lint:types": "tsc --noEmit",
    "lint:package": "publint",
    "lint:published-types": "attw --pack",
    "test": "vitest --coverage"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.7.1",
    "@nullvoxpopuli/eslint-configs": "^3.2.0",
    "@tsconfig/esm": "^1.0.4",
    "@tsconfig/strictest": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@vitest/coverage-v8": "^0.34.2",
    "eslint": "^8.47.0",
    "prettier": "^3.0.2",
    "publint": "^0.2.0",
    "turbo": "^1.10.12",
    "typescript": "^5.1.6",
    "vitest": "^0.34.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "volta": {
    "node": "18.17.1",
    "pnpm": "8.6.12"
  },
  "dependencies": {
    "capital-case": "^1.0.4"
  }
}
